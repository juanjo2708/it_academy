package Map;

import java.util.HashMap;

import ArrayList.Empleado;

public class Inici {

	public static void main(String[] args) {
		
		Empleado emp1 = new Empleado("emp 1",21,3200);
		Empleado emp2 = new Empleado("emp 2",21,3200);
		Empleado emp3 = new Empleado("emp 3",21,3200);
		Empleado emp4 = new Empleado("emp 4",21,3200);
		
		
		HashMap<String, Empleado> mapa = new HashMap<String, Empleado>();
		mapa.put("145", emp1);
		mapa.put("146", emp2);
		mapa.put("147", emp3);
		mapa.put("148", emp4);
		
		System.out.println("- "+mapa);
		System.out.println("- -----------------------------");
		System.out.println("- "+mapa.entrySet());
		
	}

}
